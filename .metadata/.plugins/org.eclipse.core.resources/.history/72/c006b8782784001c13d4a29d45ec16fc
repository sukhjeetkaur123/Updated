package prallelClassAssignment;

import org.testng.annotations.Test;

public class ParallelClass {
	@Test
	public void addValue() {
		System.out.print("this is  first method of 2nd class ");
	}
	@Test
//	parallelClassPackage.xml
	public void verifyTitle() {
		System.out.println("this is the second method of second class for check parallel execution");
	}
// u can also add threadpoll if dt want to use of xml file
//	(threadPoolSize=4,invocationAccount = 4,timeout=1000)

}
